test(): int {
    return 5;
}
main() {
    a: int = 1 + 2;
    // CHECK: 3
    printi(a);

    b: int = 5;
    c: int = a + b;
    // CHECK: 8
    printi(c);

    b = 5;
    // CHECK: 5
    printi(b);

    d: int = a * b;
    // CHECK: 15
    printi(d);

    e: int = a * b + c;
    // CHECK: 23
    printi(e);

    f: int = a + b * c;
    // CHECK: 43
    printi(f);

    g: int = test();
    // CHECK: 5
    printi(g);

    h: int = b + 1;
    // CHECK: 6
    printi(h);

    i: int = 1 + b;
    // CHECK: 6
    printi(i);
}

