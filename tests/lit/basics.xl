//---------------------------------------

func test(): i32 {
    return 5;
}
func main() {
    const a: i32 = 1 + 2;
    // CHECK: 3
    print(a);

    var b: i32 = 5;
    const c: i32 = a + b;
    // CHECK: 8
    print(c);

    b = 5;
    // CHECK: 5
    print(b);

    const d: i32 = a * b;
    // CHECK: 15
    print(d);

    const e: i32 = a * b + c;
    // CHECK: 23
    print(e);

    const f: i32 = a + b * c;
    // CHECK: 43
    print(f);

    const g: i32 = test();
    // CHECK: 5
    print(g);

    const h: i32 = b + 1;
    // CHECK: 6
    print(h);

    const i: i32 = 1 + b;
    // CHECK: 6
    print(i);
}

//---------------------------------------

func main() {
    var i: i32 = 0;
    loop {
        i = i + 1;
        if (i == 5) {
            break;
        }
    }
    // CHECK: 5
    print(i);
}

//---------------------------------------

func main() {
    // CHECK: 7
    print(1 + 2 * 3);
    // CHECK: 9
    print((1 + 2) * 3);
    // CHECK: 7
    print(1 + (2 * 3));
}

//---------------------------------------

func main() {
    const a: f32 = 3.5;
    const b: f32 = 1.2;

    // CHECK: 3.5
    print(a);
    // CHECK: 4.7
    print(a + b);
    // CHECK: 4.2
    print(a * b);
}

//---------------------------------------

func main() {
    const a: i32 = -5;
    // CHECK: -5
    print(a);

    const b: i32 = -a;
    // CHECK: 5
    print(b);

    const c: i32 = -(a + 2); // -( -5 + 2) = -(-3) = 3
    // CHECK: 3
    print(c);

    const d: i32 = -a + 2; // -(-5) + 2 = 5 + 2 = 7
    // CHECK: 7
    print(d);

    var x: i32 = 10;
    const e: i32 = -x * 2; // (-10) * 2 = -20
    // CHECK: -20
    print(e);

    const f: i32 = -(x * 2); // -(10 * 2) = -(20) = -20
    // CHECK: -20
    print(f);

    const g: i32 = -10;
    // CHECK: -10
    print(g);

    const h: i32 = 5 + -3; // 5 + (-3) = 2
    // CHECK: 2
    print(h);

    const i: i32 = 5 * -3; // 5 * (-3) = -15
    // CHECK: -15
    print(i);

    // CHECK: -2
    print(-2);
    // CHECK: 2
    print(-(-2));
}
