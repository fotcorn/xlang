// RUN: %run --split-input-file %s | %filecheck --match-full-lines %s

a() {
}
b(): int {
    return 1;
}
c(p1: int) {
}
d(p1: int): int {
    return 2;
}
e(p1: int, p2: int) {
}
f(p1: int, p2: int): int {
    return 3;
}
main() {
    a();
    // CHECK: 1
    printi(b());
    c(1);
    // CHECK: 2
    printi(d(1));
    e(1, 2);
    // CHECK: 3
    printi(f(1, 2));
}

//---------------------------------------

myfunc(): int {
    return 5;
}
main() {
    const i: int = myfunc();
    // CHECK: 5
    printi(i);
}

//---------------------------------------

struct MyStruct {
    a: int,
    b: int,
}

myfunc(): MyStruct {
    var s: MyStruct;
    s.a = 5;
    s.b = 7;
    return s;
}

main() {
    const s: MyStruct = myfunc();
    // CHECK: 5
    printi(s.a);
    // CHECK: 7
    printi(s.b);
}

//---------------------------------------

struct MyStruct {
    a: int,
    b: int,
}

myfunc(s: MyStruct) {
    // CHECK: 5
    printi(s.a);
    // CHECK: 7
    printi(s.b);
    s.a = 11;
    s.b = 12;
}

main() {
    var s: MyStruct;
    s.a = 5;
    s.b = 7;
    myfunc(s);
    // CHECK: 5
    printi(s.a);
    // CHECK: 7
    printi(s.b);
}

//---------------------------------------

struct X {
    a: int,
}
func_ref(x: *X) {
    x.a = 5;
}
func_copy(x: X) {
    x.a = 6;
}
main() {
    var x: X;
    x.a = 1;
    func_ref(x);
    // CHECK: 5
    printi(x.a);

    x.a = 2;
    func_copy(x);
    // CHECK: 2
    printi(x.a);
}
